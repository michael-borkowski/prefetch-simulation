# This simulation uses ticks in the dimension of 1s

seed 26031991001

ticks 3600					# 1 * 3600 -> 1 hour
byterate u/1000/5000				# 30-50 kB/s (up to 0.4 Mbit/s)
slot-length 180					# slots have a length of ~3 minutes
network-uptime 0.90				# 10% chance that a slot has 0 byterate
relative-jitter 0				# 
absolute-jitter 0				# 
relative-prediction-time-error 0		# relative temporal prediction error 0%
relative-prediction-amplitude-error ~/0/0.01	# relative amplitudinal prediction error 0 +- 1%
absolute-prediction-time-error ~/0/10		# absolute temporal prediction error 0 +- 10 ticks
absolute-prediction-amplitude-error 0		# absolute amplitudinal prediction error 0
look-ahead 3600					# look-ahead practically infinite

#algorithm at.borkowski.prefetchsimulation.algorithms.NullAlgorithm
#algorithm at.borkowski.prefetchsimulation.algorithms.IgnoreRatePredictionAlgorithm
#algorithm at.borkowski.prefetchsimulation.algorithms.RespectRatePredictionAlgorithm

algorithm-parameter alpha 0.9

request-series interval u/400/600 size ~/500000/60000 byterate ~/3800/100 start 1000 end 3000
request tick 3400 byterate 3600 data 480000


